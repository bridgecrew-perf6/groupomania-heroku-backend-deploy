{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "heroku",
      "port": 9090,
      "request": "attach",
      "type": "pwa-chrome",
      "address": "localhost",
      "webRoot": "${workspaceFolder}",
      "protocol": "inspector",
      "localRoot": "${workspaceFolder}",
      "remoteRoot": "/server.js"
    },
    {
      "type": "node-terminal",
      "name": "Run Script: start",
      "request": "launch",
      "command": "npm run start",
      "cwd": "${workspaceFolder}"
    }
  ]
}
